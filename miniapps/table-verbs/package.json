{
  "name": "my-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --build --force",
    "lint:code": "eslint . --ext .vue,.js,.ts --ignore-path .gitignore",
    "lint:code:fix": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "lint:code-errors": "eslint . --ext .vue,.js,.ts --ignore-path .gitignore --quiet",
    "lint:code-errors:fix": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore --quiet",
    "format": "prettier --write src/",
    "prepare": "husky",
    "pre-push": "run-p format lint:code:fix"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "pinia": "^2.1.7",
    "primevue": "^3.51.0",
    "vite-plugin-pwa": "^0.19.8",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
      "prettier --write src/"
    ]
  }
}
